<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.oa.bean">

	<class name="Vacate" table="oa_AskForLeave">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="AskForLeave_reason"/>	
		<property name="AskForLeave_type"/>
		<property name="begintime"/>
		<property name="time"/>
		<property name="initiator"/>
		<property name="sendto"/>
		<property name="sendtoName"/>
		<property name="ispass"/>
		<property name="executionid"/>
		<property name="date"/>
		
			
	</class>

</hibernate-mapping>

