<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.oa.bean">

	<class name="FlowCategory" table="oa_flowcategory">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="F_name"/>
		
		<!-- flowProcess属性，本类与FlowProcess的一对多关系 -->
		<set name="flowProcess">
			<key column="flowcategoryId"></key>
			<one-to-many class="FlowProcess"/>
		
		</set>		
	</class>

</hibernate-mapping>

