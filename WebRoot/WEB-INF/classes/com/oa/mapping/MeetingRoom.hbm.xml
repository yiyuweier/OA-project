<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.oa.bean">

	<class name="MeetingRoom" table="oa_meetingroom">
		<id name="id" type="long" column="id">
			<generator class="native"/>
		</id>
		<property name="name"  type="java.lang.String"/>
		<property name="people" type="java.lang.Integer"/>
		<property name="roomAddress"  type="java.lang.String" />	
		<property name="state"  type="java.lang.Integer" />
		
		
		
		<!-- 在一的一方要设置set，和PO里的Set集合相对应，外键就是一方的id -->
  <set name="meetingInfos" cascade="delete" inverse="true">
   <key column="meetingRoomId"></key>
   <one-to-many class="MeetingInfo"/>
  </set>
		
	</class>
</hibernate-mapping>