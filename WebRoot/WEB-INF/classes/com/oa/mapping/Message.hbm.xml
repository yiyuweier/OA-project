<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.oa.bean">

	<class name="Message" table="oa_message">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="M_name"/>	
		<property name="M_content" type="text"/>
		<property name="initiator"/>
		<property name="sendto"/>
		<property name="sendtoName"/>
		<property name="M_date"/>
		<property name="M_FJUrl"/>
		<property name="M_FJName"/>
		<property name="state"/>
		<property name="execution_id"/>
		<property name="task_id"/>
		
			
	</class>

</hibernate-mapping>

